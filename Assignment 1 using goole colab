        "## 9.Swap two numbers.\n"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "LRidTefgAcar",
        "colab_type": "code",
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 90
        },
        "outputId": "5ac441f7-481a-4d3e-9ead-66c86cb52358"
      },
      "source": [
        "a=int(input(\"Enter a number:\"))\n",
        "b=int(input(\"Enter a number:\"))\n",
        "print (\"values before Swapping\",a,b)\n",
        "temp=a\n",
        "a=b\n",
        "b=temp\n",
        "print(\"Values after Swapping\",a,b)\n"
      ],
      "execution_count": 9,
      "outputs": [
        {
          "output_type": "stream",
          "text": [
            "Enter a number:5\n",
            "Enter a number:6\n",
            "values before Swapping 5 6\n",
            "Values after Swapping 6 5\n"
          ],
          "name": "stdout"
        }
      ]
    }
